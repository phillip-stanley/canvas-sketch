{
  "name": "canvas-sketch",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm run transpile\"",
    "build": "rm -rf dist && babel src -d dist",
    "serve": "./node_modules/.bin/http-server -p 8080 -o -c-1",
    "transpile": "./node_modules/.bin/babel src -wd dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.2"
  }
}
